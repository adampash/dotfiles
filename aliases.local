# git
alias gp="git pull"
alias gpp="git pull && git push"
alias gc="git commit -v"
alias gl="git log"
alias glo="git log origin"

# dotfiles
alias dotfiles="cd ~/dotfiles"
alias vdotfiles="vi ~/dotfiles"

# code
alias code="cd ~/code"

# publish jekyll blog
alias publish="rake publish"

# ensuring rake is pointing to rake
alias rake="rake"

copy () { 
  cat "$@" | pbcopy;
}
